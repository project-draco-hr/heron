def run(command, parser, cl_args, unknown_args):
    try:
        config_overrides = utils.parse_cmdline_override(cl_args)
        new_args = ['--cluster', cl_args['cluster'], '--role', cl_args['role'], '--environment', cl_args['environ'], '--heron_home', utils.get_heron_dir(), '--config_path', cl_args['config_path'], '--config_overrides', base64.b64encode(config_overrides), '--command', 'ps']
        lib_jars = utils.get_heron_libs((jars.command_jars() + jars.statemgr_jars()))
        execute.heron_class('com.twitter.heron.command.CommandHandlerMain', lib_jars, extra_jars=[], args=new_args)
    except Exception as ex:
        print ex
        Log.error('Failed to get list of topologies')
        return False
    return True
