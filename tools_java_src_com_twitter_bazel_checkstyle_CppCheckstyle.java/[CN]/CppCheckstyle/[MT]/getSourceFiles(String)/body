{
  ExtraActionInfo info=ExtraActionUtils.getExtraActionInfo(extraActionFile);
  CppCompileInfo cppInfo=info.getExtension(CppCompileInfo.cppCompileInfo);
  return Collections2.filter(cppInfo.getSourcesAndHeadersList(),Predicates.and(Predicates.not(Predicates.containsPattern("3rdparty/")),Predicates.not(Predicates.containsPattern("config/")),Predicates.not(Predicates.containsPattern("heron/proto/"))));
}
