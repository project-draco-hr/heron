{
  try {
    graphite.flush();
  }
 catch (  Exception e) {
    LOG.log(Level.SEVERE,"Error flushing metrics to Graphite",e);
    LOG.severe("Dropping messages.");
    try {
      graphite.close();
    }
 catch (    IOException e1) {
      LOG.log(Level.SEVERE,"Error closing connection to Graphite",e1);
    }
  }
}
