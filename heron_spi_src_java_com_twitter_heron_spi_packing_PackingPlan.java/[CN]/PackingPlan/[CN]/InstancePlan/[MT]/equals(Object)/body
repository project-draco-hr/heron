{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  InstancePlan that=(InstancePlan)o;
  return getComponentName().equals(that.getComponentName()) && getTaskId() == that.getTaskId() && getComponentIndex() == that.getComponentIndex() && getResource().equals(that.getResource());
}
