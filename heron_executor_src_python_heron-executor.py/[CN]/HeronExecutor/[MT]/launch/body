def launch(self):
    ' launch '
    commands = {}
    if (self.shard == 0):
        commands = self.get_tmaster_processes()
    else:
        self.untar_if_tar()
        commands = self.get_streaming_processes()
    commands.update(self.get_heron_support_processes())
    self.do_run_and_wait(commands)
