def main():
    ' main '
    if (len(sys.argv) != 32):
        print_usage()
        sys.exit(1)
    shell_env = os.environ.copy()
    shell_env['PEX_ROOT'] = os.path.join(os.path.abspath('.'), '.pex')
    executor = HeronExecutor(sys.argv, shell_env)
    executor.prepareLaunch()
    executor.launch()
