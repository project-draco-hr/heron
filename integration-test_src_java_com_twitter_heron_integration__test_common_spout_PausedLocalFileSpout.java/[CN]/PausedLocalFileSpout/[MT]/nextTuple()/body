{
  if (br == null) {
    return;
  }
  try {
    String currentLine;
    if ((currentLine=br.readLine()) != null) {
      collector.emit(new Values(currentLine),"MESSAGE_ID");
    }
  }
 catch (  Exception e) {
    try {
      if (br != null) {
        br.close();
      }
    }
 catch (    Exception e1) {
      throw new RuntimeException("Unable to close stream reader",e1);
    }
    throw new RuntimeException("Unable to emit tuples normally",e);
  }
}
