{
  this.config=config;
  Protos.FrameworkInfo.Builder frameworkInfoBuilder=Protos.FrameworkInfo.newBuilder().setName(config.schedulerName).setCheckpoint(config.checkpoint).setRole(config.role).setFailoverTimeout(config.failoverTimeoutSeconds).setUser(config.user).setHostname(config.hostname);
  if (frameworkId != null) {
    frameworkInfoBuilder.setId(frameworkId);
  }
  if (config.authenticationPrincipal != null) {
    frameworkInfoBuilder.setPrincipal(config.authenticationPrincipal);
  }
  frameworkInfo=frameworkInfoBuilder.build();
  return this;
}
