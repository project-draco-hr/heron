{
  Assert.assertEquals("${HERON_SANDBOX_BIN}/heron-executor",ConfigDefaults.get("SANDBOX_EXECUTOR_BINARY"));
  Assert.assertEquals("${HERON_SANDBOX_BIN}/heron-stmgr",ConfigDefaults.get("SANDBOX_STMGR_BINARY"));
  Assert.assertEquals("${HERON_SANDBOX_BIN}/heron-tmaster",ConfigDefaults.get("SANDBOX_TMASTER_BINARY"));
  Assert.assertEquals("${HERON_SANDBOX_BIN}/heron-shell",ConfigDefaults.get("SANDBOX_SHELL_BINARY"));
  Assert.assertEquals("${HERON_SANDBOX_BIN}/heron-python-instance",ConfigDefaults.get("SANDBOX_PYTHON_INSTANCE_BINARY"));
  Assert.assertEquals("heron.jars.scheduler","${HERON_LIB}/scheduler/heron-scheduler.jar",ConfigDefaults.get("SCHEDULER_JAR"));
}
