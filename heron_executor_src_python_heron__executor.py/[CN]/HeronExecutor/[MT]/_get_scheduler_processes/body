def _get_scheduler_processes(self):
    retval = {}
    scheduler_cmd = ['java', '-cp', self.scheduler_classpath, 'com.twitter.heron.scheduler.SchedulerMain', self.cluster, self.role, self.environ, self.topology_name, self.topology_jar_file, self.scheduler_port]
    retval['heron-tscheduler'] = scheduler_cmd
    return retval
