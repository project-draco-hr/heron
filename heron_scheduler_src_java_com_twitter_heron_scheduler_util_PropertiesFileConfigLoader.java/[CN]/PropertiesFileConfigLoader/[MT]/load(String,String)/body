{
  Properties workingCopy=new Properties();
  if (ConfigLoaderUtils.loadPropertiesFile(workingCopy,propertiesFile) && ConfigLoaderUtils.applyPropertyOverride(workingCopy,propertyOverride) && ConfigLoaderUtils.applyConfigPropertyOverride(workingCopy)) {
    properties.putAll(workingCopy);
    addDefaultProperties();
    if (isVerbose()) {
      LOG.info("Config properties parsed: \n" + properties);
    }
    return true;
  }
  return false;
}
