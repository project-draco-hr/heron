{
  ++nItems;
  if (nItems % 10000 == 0) {
    long latency=System.currentTimeMillis() - startTime;
    System.out.println("Done " + nItems + " in "+ latency);
    GlobalMetrics.incr("selected_items");
  }
 else   if (nItems % 2 == 0) {
    collector.fail(tuple);
  }
 else {
    collector.ack(tuple);
  }
}
