{
  boolean isPortsEnough=false;
  for (  Range portRange : portsHold) {
    if (portRange.rangeEnd - portRange.rangeStart + 1 > needed.ports) {
      isPortsEnough=true;
      break;
    }
  }
  return isPortsEnough && (this.cpu >= needed.cpu) && (this.mem >= needed.mem)&& (this.disk >= needed.disk);
}
