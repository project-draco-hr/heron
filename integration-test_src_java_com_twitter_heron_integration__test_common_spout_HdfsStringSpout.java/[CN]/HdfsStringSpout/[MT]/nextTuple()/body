{
  if (br == null) {
    return;
  }
  try {
    String line="";
    if ((line=br.readLine()) != null) {
      collector.emit(new Values(line));
    }
 else {
      br.close();
      br=null;
    }
  }
 catch (  Exception e) {
    try {
      br.close();
    }
 catch (    Exception e1) {
      throw new RuntimeException("Unable to close stream reader",e1);
    }
    throw new RuntimeException("Unable to emit tuples normally",e);
  }
}
