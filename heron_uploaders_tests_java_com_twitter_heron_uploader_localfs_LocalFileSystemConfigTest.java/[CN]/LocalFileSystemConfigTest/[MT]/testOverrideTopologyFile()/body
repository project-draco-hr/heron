{
  LocalFileSystemUploader uploader=new LocalFileSystemUploader();
  String overrideDirectory="/users/twitter";
  Config config=Config.newBuilder().putAll(getDefaultConfig()).put(LocalFileSystemKeys.FILE_SYSTEM_DIRECTORY,overrideDirectory).build();
  uploader.initialize(config);
  String destFile=Paths.get(overrideDirectory,"topology." + Context.topologyPackageType(config)).toString();
  Assert.assertEquals(uploader.getTopologyFile(),destFile);
}
