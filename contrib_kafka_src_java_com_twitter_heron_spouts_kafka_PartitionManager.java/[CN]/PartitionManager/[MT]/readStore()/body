{
  try {
    byte[] data=storage.readPKey(partition.toString());
    if (data != null && data.length > 0) {
      return (Map<Object,Object>)JSONValue.parse(new String(data,"UTF-8"));
    }
  }
 catch (  Exception e) {
    LOG.error("Failed to read state in primary store for " + partition);
  }
  return null;
}
