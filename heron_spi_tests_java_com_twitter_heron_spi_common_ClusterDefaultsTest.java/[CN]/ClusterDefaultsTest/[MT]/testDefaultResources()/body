{
  Config props=ClusterDefaults.getDefaultResources();
  Assert.assertEquals(Long.valueOf(Defaults.STMGR_RAM),Context.stmgrRam(props));
  Assert.assertEquals(Double.valueOf(Defaults.INSTANCE_CPU),Context.instanceCpu(props),0.001);
  Assert.assertEquals(Long.valueOf(Defaults.INSTANCE_RAM),Context.instanceRam(props));
  Assert.assertEquals(Long.valueOf(Defaults.INSTANCE_DISK),Context.instanceDisk(props));
}
