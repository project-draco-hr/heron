{
  PackingPlans.PackingPlan.Builder builder=PackingPlans.PackingPlan.newBuilder().setId(packingPlan.getId()).setResource(builder(packingPlan.getResource()));
  for (  PackingPlan.ContainerPlan containerPlan : packingPlan.getContainers().values()) {
    builder.addContainerPlans(builder(containerPlan));
  }
  return builder.build();
}
