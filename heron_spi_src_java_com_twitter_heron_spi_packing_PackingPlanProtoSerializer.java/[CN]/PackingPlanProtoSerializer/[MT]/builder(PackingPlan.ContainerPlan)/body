{
  PackingPlans.ContainerPlan.Builder builder=PackingPlans.ContainerPlan.newBuilder().setId(containerPlan.getId()).setResource(builder(containerPlan.getResource()));
  for (  PackingPlan.InstancePlan instancePlan : containerPlan.getInstances().values()) {
    builder.addInstancePlans(builder(instancePlan));
  }
  return builder;
}
