{
  PowerMockito.mockStatic(ShellUtility.class);
  Mockito.when(ShellUtility.runSyncProcess(Matchers.anyBoolean(),Matchers.anyBoolean(),Matchers.anyString(),Matchers.any(StringBuilder.class),Matchers.any(StringBuilder.class),Matchers.any(File.class))).thenReturn(0);
  String packageName="someTopologyPkg.tar";
  LocalLauncher launcher=Mockito.spy(LocalLauncher.class.newInstance());
  Assert.assertTrue(launcher.untarPackage(packageName,""));
  String expectedUntarCmd=String.format("tar -xvf %s",packageName);
  PowerMockito.verifyStatic();
  ShellUtility.runSyncProcess(Matchers.anyBoolean(),Matchers.anyBoolean(),Matchers.eq(expectedUntarCmd),Matchers.any(StringBuilder.class),Matchers.any(StringBuilder.class),Matchers.any(File.class));
}
