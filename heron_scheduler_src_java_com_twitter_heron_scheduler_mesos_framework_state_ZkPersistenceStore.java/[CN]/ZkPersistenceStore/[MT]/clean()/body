{
  LOG.info("Cleaning all unneeded meta-data");
  LOG.info("Doing some cleaning");
  try {
    client.delete().deletingChildrenIfNeeded().forPath(path);
  }
 catch (  Exception e) {
    LOG.info("Unable to clean");
    return false;
  }
  LOG.info("Clean done");
  LOG.info("Closing the CuratorClient");
  client.close();
  return true;
}
