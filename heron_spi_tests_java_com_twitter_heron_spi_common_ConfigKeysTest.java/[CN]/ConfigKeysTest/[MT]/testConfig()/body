{
  Assert.assertEquals("heron.config.cluster",ConfigKeys.get("CLUSTER"));
  Assert.assertEquals("heron.config.role",ConfigKeys.get("ROLE"));
  Assert.assertEquals("heron.config.environ",ConfigKeys.get("ENVIRON"));
  Assert.assertEquals("heron.config.verbose",ConfigKeys.get("VERBOSE"));
  Assert.assertEquals("heron.config.path",ConfigKeys.get("CONFIG_PATH"));
  Assert.assertEquals("heron.config.property",ConfigKeys.get("CONFIG_PROPERTY"));
}
