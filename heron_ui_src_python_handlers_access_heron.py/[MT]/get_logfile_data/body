@tornado.gen.coroutine
def get_logfile_data(cluster, environ, topology, instance, offset, length):
    request_url = tornado.httputil.url_concat(create_url(LOGFILE_DATA_URL_FMT), dict(cluster=cluster, environ=environ, topology=topology, instance=instance, offset=offset, length=length))
    raise tornado.gen.Return(yield fetch_url_as_json(request_url))
