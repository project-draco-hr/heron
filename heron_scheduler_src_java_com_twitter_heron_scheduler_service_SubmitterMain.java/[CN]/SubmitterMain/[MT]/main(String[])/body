{
  String topologyPackage=args[0];
  String submitterConfigLoader=args[1];
  String submitterConfigFile=args[2];
  String submitterConfigOverrideEncoded=args[3];
  String topologyDefnFile=args[4];
  heronInternalsFile=args[5];
  originalPackageFile=args[6];
  String configOverride=new String(DatatypeConverter.parseBase64Binary(submitterConfigOverrideEncoded),Charset.forName("UTF-8"));
  if (!submitTopology(topologyPackage,submitterConfigLoader,submitterConfigFile,configOverride,TopologyUtility.getTopology(topologyDefnFile))) {
    Runtime.getRuntime().exit(1);
  }
}
