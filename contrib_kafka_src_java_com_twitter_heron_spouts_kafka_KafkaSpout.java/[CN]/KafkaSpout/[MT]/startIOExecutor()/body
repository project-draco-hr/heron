{
  IOExecutorService.FnParam<Boolean,TransferCollector.EmitData,PartitionCoordinator> fn=new IOExecutorService.FnParam<Boolean,TransferCollector.EmitData,PartitionCoordinator>(){
    @Override public Boolean apply(    BlockingQueue<TransferCollector.EmitData> stream,    PartitionCoordinator context){
      for (      PartitionManager manager : context.getMyManagedPartitions()) {
        manager.next((TransferCollector)stream);
      }
      return true;
    }
  }
;
  this.executor=new IOExecutorService.SingleThreadIOExecutorService<>(fn,coordinator,transferCollector);
  executor.scheduleContinuously();
}
