{
  if (!hasId()) {
    return false;
  }
  if (!hasName()) {
    return false;
  }
  if (!hasState()) {
    return false;
  }
  for (int i=0; i < getSpoutsCount(); i++) {
    if (!getSpouts(i).isInitialized()) {
      return false;
    }
  }
  for (int i=0; i < getBoltsCount(); i++) {
    if (!getBolts(i).isInitialized()) {
      return false;
    }
  }
  if (hasTopologyConfig()) {
    if (!getTopologyConfig().isInitialized()) {
      return false;
    }
  }
  return true;
}
