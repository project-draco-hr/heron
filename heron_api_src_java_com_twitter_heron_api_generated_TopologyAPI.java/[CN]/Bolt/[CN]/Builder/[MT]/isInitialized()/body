{
  if (!hasComp()) {
    return false;
  }
  if (!getComp().isInitialized()) {
    return false;
  }
  for (int i=0; i < getInputsCount(); i++) {
    if (!getInputs(i).isInitialized()) {
      return false;
    }
  }
  for (int i=0; i < getOutputsCount(); i++) {
    if (!getOutputs(i).isInitialized()) {
      return false;
    }
  }
  return true;
}
