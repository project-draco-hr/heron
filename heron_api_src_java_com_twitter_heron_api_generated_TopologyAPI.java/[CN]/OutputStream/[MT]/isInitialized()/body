{
  byte isInitialized=memoizedIsInitialized;
  if (isInitialized != -1)   return isInitialized == 1;
  if (!hasStream()) {
    memoizedIsInitialized=0;
    return false;
  }
  if (!hasSchema()) {
    memoizedIsInitialized=0;
    return false;
  }
  if (!getStream().isInitialized()) {
    memoizedIsInitialized=0;
    return false;
  }
  if (!getSchema().isInitialized()) {
    memoizedIsInitialized=0;
    return false;
  }
  memoizedIsInitialized=1;
  return true;
}
