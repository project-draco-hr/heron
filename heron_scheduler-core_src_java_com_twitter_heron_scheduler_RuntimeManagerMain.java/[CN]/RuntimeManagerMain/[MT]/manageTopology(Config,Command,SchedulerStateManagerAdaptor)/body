{
  Config runtime=Config.newBuilder().put(Keys.topologyName(),Context.topologyName(config)).put(Keys.schedulerStateManagerAdaptor(),adaptor).build();
  ISchedulerClient schedulerClient=new SchedulerClientFactory(config,runtime).getSchedulerClient();
  if (schedulerClient == null) {
    throw new IllegalArgumentException("Failed to initialize scheduler client");
  }
  RuntimeManagerRunner runtimeManagerRunner=new RuntimeManagerRunner(config,runtime,command,schedulerClient);
  boolean ret=runtimeManagerRunner.call();
  return ret;
}
