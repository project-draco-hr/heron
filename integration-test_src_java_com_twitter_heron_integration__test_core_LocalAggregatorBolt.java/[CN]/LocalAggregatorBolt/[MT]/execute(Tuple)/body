{
  try {
    String data=tuple.getString(0);
    bw.write(data);
    bw.newLine();
  }
 catch (  IOException e) {
    try {
      if (bw != null) {
        bw.close();
      }
    }
 catch (    IOException e1) {
      throw new RuntimeException("Unable to close stream writer",e1);
    }
    throw new RuntimeException("Unable to write to file or emit tuples",e);
  }
}
