def create_parser(subparsers):
    parser = subparsers.add_parser('activate', help='Activate a topology', usage='%(prog)s [options] config-overrides topology-name', add_help=False)
    args.add_titles(parser)
    args.add_config_overrides(parser)
    args.add_topology(parser)
    args.add_config(parser)
    args.add_verbose(parser)
    parser.set_defaults(subcommand='activate')
    return parser
